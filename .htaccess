RewriteEngine On
RewriteCond %{HTTPS} !=on
RewriteRule ^(.*)$ https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301,NE]
RewriteCond %{http_host} ^la-chouette-agence.stephane-malho.fr/ [nc]
RewriteRule ^(.*)$ https://www.la-chouette-agence.stephane-malho.fr/$1 [r=301,nc]

## contr√¥le du cache navigateur - Expire headers
<IfModule mod_expires.c>
	ExpiresActive On
	ExpiresDefault "access plus 7200 seconds"
	ExpiresByType ../assets/css/et-line.css                             "access plus 1 year"
	ExpiresByType ../assets/css/style.css                               "access plus 1 year"
	ExpiresByType ../assets/js/blocs.js 			                    "access plus 1 year"
	ExpiresByType ../assets/js/gmaps.js 		                        "access plus 1 year"
	ExpiresByType ../assets/js/jqBootstrapValidation.js        	        "access plus 1 year"
	ExpiresByType ../assets/img/main-logo-min-adobespark.png	        "access plus 1 year"
	ExpiresByType ../assets/img/circle-logo-min.png         	        "access plus 1 year"
    ExpiresByType ../assets/img/desk-work-min-adobespark.jpeg	        "access plus 1 year"
	ExpiresByType ../assets/img/design-worker-min-adobespark.jpeg	    "access plus 1 year"
	ExpiresByType ../assets/img/livres-journal-min-adobespark.jpeg	    "access plus 1 year"
	ExpiresByType ../assets/img/femme-photographe-min-adobespark.jpeg	"access plus 1 year"
	ExpiresByType ../assets/img/materiel-tech-adobespark.jpeg       	"access plus 1 year"
	ExpiresByType ../assets/img/creatif-desk-adobespark.jpg	            "access plus 1 year"
	ExpiresByType ../assets/img/logo-title-min.jpeg	                    "access plus 1 year"
</IfModule>

Header set X-Content-Type-Options "nosniff"
Header always set X-Frame-Options "SAMEORIGIN"
